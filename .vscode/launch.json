    "version": "0.2.0",
    "configurations": [ 
        {
        "name": "Python: Current File (integrated)",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "stopOnEntry": false,
        "pythonPath": "/opt/conda/envs/myenv/bin/python",
        "cwd": "${workspaceRoot}",
        "env": {"PYTHONPATH": "/opt/conda/envs/myenv/bin/python:src"}
        }
    ]
}
